@{
	Layout = null;
}
<html>
<head>
	<title>Channels Subscribed To</title>
	<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
	<script>
		$(document).ready(function() {
			$.get(`/Main/GetSubscribedChannels`, (data)=>{
				$("#channels").html(data);
			});
		});
	</script>
</head>
<body>
	<h2>Channels Subscribed To</h2>
	<div id="channels"></div>
</body>
</html>